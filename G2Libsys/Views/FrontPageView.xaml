<local:BasePage x:Class="G2Libsys.Views.FrontPageView"
                x:TypeArguments="vm:FrontPageViewModel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:G2Libsys.Views"
        xmlns:vm="clr-namespace:G2Libsys.ViewModels"
        mc:Ignorable="d">
    <Grid Background="GreenYellow">
        <Button Content="Customer"
                Command="{Binding NavigateToVM}"
                CommandParameter="{x:Type vm:TestVM}"
                />
    </Grid>
</local:BasePage>
