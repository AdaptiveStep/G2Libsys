<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:G2Libsys"
    xmlns:vm="clr-namespace:G2Libsys.ViewModels"
    xmlns:vs="clr-namespace:G2Libsys.Views">

    <!--  Match ViewModels To Views  -->
    <DataTemplate DataType="{x:Type vm:FrontPageViewModel}">
        <vs:FrontPageView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TestVM}">
        <vs:TestView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <vs:LoginView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AdminViewModel}">
        <vs:AdminView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LibraryMainViewModel}">
        <vs:LibraryMainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:LibraryObjectViewModel}">
        <vs:LibraryObjectView />
    </DataTemplate>

</ResourceDictionary>